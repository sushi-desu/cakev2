{
  "manifest_version": 2,
  "name": "cakev2",
  "version": "2.0.1",
  "browser_action": {
    "default_title": "Run Cake",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
